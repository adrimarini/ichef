<h1>Edit Image</h1>

<%= form_for @image do |f| %>

    <div>
        <%= f.label :url %>
        <%= f.text_field :url %>
    </div>


    <%= f.submit "Submit changes" %>

<% end %>

<%= link_to "Back", images_path %>
<%= link_to "Delete", image_path, method: :delete, data: { confirm: "Are you sure you want to delete this image?"} %>
